{
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://reviewhub-website-manus-latest.onrender.com/:path*" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
